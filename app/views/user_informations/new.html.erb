<h1>New User Information</h1>
<% if @user_information.errors.any? %>
<% @user_information.errors.full_messages.each do |msg| %>
<div class="alert alert-danger"><%= msg %></div>
<%end%>
<%end%>

<%= render 'form', user_information: @user_information %>

<%= link_to 'Back', user_informations_path, { type:"button", class:"btn btn-info" } %>
